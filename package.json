    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/65455e96-7992-425f-44e4-30f3e7d7f900/preview",
    "publish": true
  },
  "dependencies": {
    "@mfullbrook/call-kaira-frontend-commons": "file:../frontend-commons",
    "@hey-api/openapi-ts": "^0.85.2",
    "@react-router/fs-routes": "^7.12.0",
    "@tanstack/react-query": "^5.90.19",
    "isbot": "5.1.26",
    "lucide-react": "^0.545.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.12.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.21.1",
    "@react-router/dev": "^7.12.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "24.0.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "tailwindcss": "^4.1.14",
    "typescript": "5.8.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.0",
    "wrangler": "^4.59.3"
  },
  "scripts": {
    "build": "react-router build",
    "build:production": "react-router build --mode production",
    "build:staging": "react-router build --mode staging",
    "cf-typegen": "wrangler types && react-router typegen",
    "check": "tsc && react-router build && wrangler deploy --dry-run",
    "clean": "rm -rf .react-router .wrangler build",
    "deploy:production": "npm run build:production && wrangler deploy --env=\"\"",
    "deploy:staging": "npm run build:staging && wrangler deploy --env=\"\"",
    "dev": "react-router dev",
    "dev:production": "react-router dev --env production",
    "dev:staging": "react-router dev --env staging",
    "postinstall": "npm run cf-typegen",
    "preview": "npm run build && vite preview",
    "typecheck": "npm run typegen && tsc -b",
    "openapi-ts": "openapi-ts"
  },
  "type": "module"
}
